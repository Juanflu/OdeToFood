@page
@model ListModel
@{
}

<h1>Restaurants</h1>

<table class="table">
    @foreach (var resturant in Model.Restaurants)
    {
        <tr>
            <td>@resturant.Name</td>
            <td>@resturant.Location</td>
            <td>@resturant.Cuisine</td>
        </tr>
    }
</table>

<div>@Model.Message</div>